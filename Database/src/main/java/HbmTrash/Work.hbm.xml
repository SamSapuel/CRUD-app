<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="Model">
    <class name="Work" table="Work">
        <meta attribute = "class-description">
            This class contains the employee detail.
        </meta>

        <id name = "employee" type = "int" column = "Employee">
            <generator class="native"/>
        </id>


        <property name = "department" column = "Department" type = "int"/>
        <property name = "chairNumber" column = "Chair_number" type = "int"/>
        <property name = "cabinetNumber" column = "Cabinet_number" type = "int"/>


    </class>
</hibernate-mapping>